<?xml version="1.0" encoding="utf-8"?>
<Configuration xmlns="http://gridpack.pnnl.gov/1.0">
  <Time>
    <Start>@START@</Start>
    <Limit>
      <Duration>0.1</Duration>
    </Limit>
    <Step>0.001</Step>
    <Mode>Steady</Mode>
  </Time>
  <Fluid>
    <Density>997.561</Density>
    <Viscosity>0.0008887</Viscosity>
  </Fluid>
  <Option>
    <Gradient>
      <Kind>Guass</Kind>
      <Iterate>2</Iterate>
    </Gradient>
    <TimeStepping>Euler</TimeStepping>
    <Hydro>
      <Enable>On</Enable>
      <Iterate>5</Iterate>
      <ConvergeMass>1e-14</ConvergeMass>
      <RelaxPressure>0.10</RelaxPressure>
      <RelaxVelocity>0.50</RelaxVelocity>
      <DeltaP2>On</DeltaP2>
    </Hydro>
    <SchemeVelocity>
      <Kind>CDS</Kind>
      <Value>0.0</Value>
    </SchemeVelocity>
    <Solver>
      <RelativeTolerance>1.0e-02</RelativeTolerance>
      <AbsoluteTolerance>1.0e-30</AbsoluteTolerance>
      <Verbose>No</Verbose>
      <Iterate>1000</Iterate>
    </Solver>
    <DefaultVelocity>
      <X>0.0</X>
      <Y>0.0</Y>
      <Z>0.0</Z>
    </DefaultVelocity>
  </Option>
  <Mesh>
    <Kind>CGNS</Kind>
    <File>mesh/@NAME@.cgns</File>
  </Mesh>
  <BCList>
    <BC>
      <Name>inlet1</Name>
      <Info>
        <Velocity>
          <Constant>1.4189e-5</Constant>
        </Velocity>
      </Info>
    </BC>
    <BC>
      <Name>inlet2</Name>
      <Info>
        <Velocity>
          <Constant>1.4189e-5</Constant>
        </Velocity>
      </Info>
    </BC>
    <BC>
      <Name>outlet</Name>
      <Info>
        <Scalar>
          <Name>press</Name>
          <Value>
            <Constant>1.4189e-5</Constant>
          </Value>
        </Scalar>
      </Info>
    </BC>
  </BCList>
  <Output>
    <CGNS>@NAME@-out.cgns</CGNS>
    <Interval>10</Interval>
  </Output>
  <BoundaryOutput>
    <Enable>On</Enable>
    <Name>@NAME@-out</Name>
    <Interval>1</Interval>
    <Select>
      <Only>Inlet</Only>
      <Only>Outlet</Only>
    </Select>
  </BoundaryOutput>

</Configuration>
